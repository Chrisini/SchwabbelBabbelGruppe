
#include <stdlib.h>
#include <stdio.h>
#include <string.h>

int main (void)
{
  char szInput [256];
  char * pEnd;
  unsigned long ul;
  
  int c = 7;
  float b;
  
  b = c /3;
  
  unsigned char str[15] = "Blueberry";
  
  unsigned char val[9] = {1,2,3};
  int i;
  for(i =2; i<10; i+=2){
	  val[i-2] +=i;
	  
  }
  val[i] = '\0';
  printf("i-2: %c\n", val*);
  
  printf ("Enter an integer value: %f %d", b, strlen(str));
  // gets (szInput);
  ul = strtol (szInput,&pEnd,5);
  printf ("Value entered: %lu. Its double: %lu\n",ul,ul*2);
  return 0;
}


